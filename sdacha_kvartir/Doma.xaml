<UserControl x:Class="sdacha_kvartir.Doma"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:sdacha_kvartir"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="150">
    <Grid>
        <Border Background="White" VerticalAlignment="Center"  CornerRadius="10" Height="90" Visibility="Visible" Grid.Column="0" Margin="5,0,5,0" Grid.ColumnSpan="2">
            <Border.Effect>
                <DropShadowEffect BlurRadius="20" Color="Black" ShadowDepth="0">
                </DropShadowEffect>
            </Border.Effect>
        </Border>
        <Button x:Name="home_btn" Click="Home_btn_Click"/>
        <Label x:Name="home_l" Content="дом" Margin="15,9,91,66"/>
    </Grid>
</UserControl>
